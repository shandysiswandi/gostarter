{
  "version": "0.2",
  "language": "en,id-id",
  "languageSettings": [
    {
      "languageId": "go",
      "allowCompoundWords": false,
      "includeRegExpList": [
        "CStyleComment",
        "string"
      ],
      "ignoreRegExpList": [
        // ignore multiline imports
        "import\\s*\\((.|[\r\n])*?\\)",
        // ignore single line imports
        "import\\s*.*\".*?\"",
        // ignore go generate directive
        "//\\s*go:generate.*",
        // ignore no-linter directive
        "//\\s*nolint:.*"
      ]
    }
  ],
  "ignorePaths": [
    "cspell.json",
    ".dockerignore",
    ".mockery.yml",
    ".golangci.yml",
    "api/gqlgen.yaml",
    "api/buf.yaml",
    "api/buf.lock",
    "api/buf.gen.yaml",
    "api/gen-gql/**",
    "api/gen-proto/**",
    "config/**"
  ],
  "words": [
    "bufbuild",
    "cespare",
    "coverprofile",
    "dbops",
    "distroless",
    "dylib",
    "genpkey",
    "goerror",
    "golangci",
    "googlepubsub",
    "GOPATH",
    "gostarter",
    "gqlgen",
    "healthcheck",
    "HMACSHA",
    "httprouter",
    "idletime",
    "installsuffix",
    "keygen",
    "Koanf",
    "ldflags",
    "lzma",
    "mockz",
    "mysqladmin",
    "nonroot",
    "otelcol",
    "otlp",
    "pkeyopt",
    "Postgre",
    "pressly",
    "protovalidate",
    "pubout",
    "qualitygate",
    "redismock",
    "redispubsub",
    "shandysiswandi",
    "sslmode",
    "todos",
    "usecase",
    "vektra"
  ]
}
